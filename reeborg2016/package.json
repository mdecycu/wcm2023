{
  "name": "reeborg",
  "version": "1.0.0",
  "private": true,
  "description": "Reeborg's World engine",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-spec": "tape test/**/*.js | tap-spec",
    "test": "tape test/**/*.js | tap-diff",
    "test-dot": "tape test/**/*.js | tap-dot",
    "test-raw": "tape test/**/*.js",
    "build": "tape test/**/*.js | tap-spec && browserify ./src/js/index.js -o ./build/reeborg.js",
    "only-build": "browserify ./src/js/index.js -o ./build/reeborg.js",
    "jsdoc": "jsdoc -c jsdoc_conf.json",
    "madge": "madge --image graph.png src/js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aroberge/reeborg.git"
  },
  "author": "Andr√© Roberge",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/aroberge/reeborg/issues"
  },
  "homepage": "https://github.com/aroberge/reeborg#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "ink-docstrap": "^1.1.0",
    "mock-require": "^1.2.1",
    "silencer": "0.0.2",
    "tap-diff": "^0.1.1",
    "tap-dot": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  }
}
