{
    "MY COMMENT": "I am using a heavily modified version of the ink-docstrap template",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [ "src/js"]
    },
    "plugins": ["plugins/markdown"],
    "markdown": {
        "tags": ["summary"]
    },
    "opts": {
        "destination": "./doc_js/",
        "recurse": true,
        "readme": "src/js/README.md",
        "template": "./node_modules/ink-docstrap/template"
    },
    "templates": {
      "theme": "reeborg"
    }
}
